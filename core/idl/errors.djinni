
ErrorCode = enum {
    # Device errors

    # Wallet errors

    # User land errors
    cancelled_by_user;


    # Others
    invalid_base58_format;
    invalid_checksum;
    invalid_version;

    # DeterministicPublicKey
    private_derivation_not_supported;

    # Generic
    runtime_error;
    out_of_range;
    illegal_argument;
    illegal_state;
    null_pointer;
    unsupported_operation;
    unknown;
}

Error = record {
    code: ErrorCode;
    message: string;
}